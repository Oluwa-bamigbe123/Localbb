@model IEnumerable<LocalBetBiga.Models.Entities.Retrival>

@{ Layout = "_AdminDashboardLayout"; }


<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InventorySystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>

@{ ViewData["Title"] = "Title"; }

<div class="text-center " style="margin-top: 50px">
    <h2 class="text-primary">RETRIVAL LIST</h2>
</div>

@if (Model.Count() > 0)
{

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Manager UserName</th>
                <th>Name Of Agent</th>
                <th>Name Of Equipment Retrieved</th>
                <th>Name Of Agent Re-Assigned To</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
<tr>
    <td>@obj.Manager.UserName</td>
    <td>@obj.NameOfAgent</td>
    <td>@obj.NameOfEquipmentRetrieved</td>
    @if (obj.NameOfAgentReassignedTo == null)
    {
<td><p class="text-danger"> empty </p></td> }
                    else
                    {
<td>@obj.NameOfAgentReassignedTo</td>}


    @*<td>

            <a asp-action="Edit" asp-route-id="@obj.Id" class="btn btn-warning">
                Edit
            </a>
        </td>*@





</tr>}


        </tbody>
    </table>
</div> <div class="text-center">

    <div class="card-footer foots">
        <div class="form-group">
            <a asp-action="ExportToExcel" class="btn btn-success">
                Export
            </a>
        </div>
    </div>
</div>


}
else
{
<p> Oops No Retrivals  Yet</p>
}



