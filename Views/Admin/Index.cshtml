@model IEnumerable<LocalBetBiga.Models.Entities.Admin>


<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InventorySystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>

@{ ViewData["Title"] = "Title"; }

<div class="text-center " style="margin-top: 50px">
    <h2 class="text-primary">Admin List</h2>
</div>

@if (Model.Count() > 0)
{

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Phone Number</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td>@obj.FirstName</td>
                        <td>@obj.LastName</td>
                        <td>@obj.Email</td>
                        <td>@obj.PhoneNumber</td>


                        @*<td>

                            <a asp-action="Edit" asp-route-id="@obj.Id" class="btn btn-warning">
                                Edit
                            </a>
                        </td>*@

                       

                    </tr>

                }
            </tbody>
        </table>
    </div>



}
else
{
    <p> Oops No Admin  Yet</p>
}



