@model IEnumerable<LocalBetBiga.Models.Entities.AdminEquipmentDistribution>

@{ Layout = "_ManagerDashboardLayout";
}


<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InventorySystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>

@{ ViewData["Title"] = "Title"; }

<div class="text-center " style="margin-top: 50px">
    <h2 class="text-primary">History</h2>
</div>

@if (Model.Count() > 0)
{

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th>Equipment Type</th>
                    <th>Equipment Brand</th>
                    <th>Number Of Equipment Received</th>
                    <th>Date Received</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td>@obj.Equipments.EquipmentType</td>
                        <td>@obj.Equipments.Brand</td>
                        <td>@obj.NumberOfEquipmentAssigned</td>
                        <td>@obj.DateAssigned</td>






                    </tr>

                }
            </tbody>
        </table>
    </div>


}
else
{
    <p> Oops No Activity  Yet</p>
}

